
<% include partialsdetail/headers.ejs %>

<div class="mt-5">
<title>Index</title>
</head>
<body>
 
  
  <form action="" method="post">
  <div class="container">
    <h1>Profile</h1>
    
    
    <div class="form-group row">
      <label for="email" class="col-sm-2 col-form-label">Email</label>
        
        <div class="col-sm-10">
          <input type="text" class="form-control" id="email" name="nmemail" placeholder="Email">
        </div>
    </div>
    
    <div class="form-group row">
        <label for="name" class="col-sm-2 col-form-label">Name</label>
          
          <div class="col-sm-10">
            <input type="text" class="form-control" id="name" name="nmname" placeholder="Name">
          </div>
      </div>
      
      <div class="form-group row">
        <label for="position" class="col-sm-2 col-form-label">Position</label>
          
          <div class="col-sm-10">
            <input class="form-check-input" type="radio" name="manager" id="manager" value="manager" checked>
                <label class="form-check-label" for="manager">
                Manager
                </label>
          <br>
            <input class="form-check-input" type="radio" name="software" id="software" value="software">
                <label class="form-check-label" for="software">
                Software Developer
                </label>
                <br>

            <input class="form-check-input" type="radio" name="quality" id="quality" value="quality" disabled>
                <label class="form-check-label" for="quality">
                Quality Assurance
                </label>
          </div>
      </div>
   
      <div class="form-group row">
        <label for="type" class="col-sm-2 col-form-label">Type</label>
          
          <div class="col-sm-10">
            <input class="form-check-input" type="checkbox" name="type" id="type" value="type">
                <label class="form-check-label" for="type">
                Fulltime
                </label>
          </div>
      </div>

    
    <div class="form-group row">
      <button type="submit" class="btn btn-primary" style="border-radius: 20px;">Update</button>
    </div>
      
    </form>
  </div>

    

</form>
  